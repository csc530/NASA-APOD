<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="525.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="space.nasa.spaceapi.controllers.apodController">
   <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <VBox alignment="CENTER" prefHeight="50.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="title" text="Label" />
                  <Label fx:id="date" text="Label" />
               </children>
            </VBox>
         </top>
         <left>
            <Button mnemonicParsing="false" onAction="#prev" prefWidth="75.0" text="Button" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button mnemonicParsing="false" onAction="#next" prefWidth="75.0" text="Button" BorderPane.alignment="CENTER" />
         </right>
         <center>
            <ImageView fx:id="image" cache="true" fitHeight="325.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <viewport>
                  <Rectangle2D />
               </viewport>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin></ImageView>
         </center>
         <bottom>
            <VBox alignment="CENTER" prefHeight="50.0" spacing="15.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="expl" alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="100.0" text="Label" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS" wrapText="true" VBox.vgrow="ALWAYS" />
                  <Button mnemonicParsing="false" onAction="#back" text="Button" />
               </children>
            </VBox>
         </bottom>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
